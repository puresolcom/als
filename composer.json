{
  "name": "awok/als",
  "description": "Awok Logistics API.",
  "keywords": [
    "awok",
    "logistics",
    "als"
  ],
  "license": "PRIVATE",
  "type": "project",
  "require": {
    "php": ">=5.6.4",
    "laravel/lumen-framework": "5.4.*",
    "vlucas/phpdotenv": "~2.2",
    "firebase/php-jwt": " ^4.0",
    "prettus/l5-repository": "^2.6",
    "league/fractal": "^0.16.0"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "~5.0",
    "mockery/mockery": "~0.9"
  },
  "autoload": {
    "psr-4": {
      "ALS\\": "ALS/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/",
      "database/"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
